include_once('news.features.inc');

/*
* hook_menu
*/
function news_menu() {
  $items = array();
 
  $items['community/%group_name/news'] = array(
    'title' => 'news',
    'page callback' => 'views_page',
    'page arguments' => array('news', 'page', 1),
    'access callback' => TRUE,
    'type' => MENU_NORMAL_ITEM,
    'menu_name' => 'menu-communities-menu',
    'weight' => 1,
  );

  return $items;
}
