name = "wiki_og"
description = "override wiki_core to add communities features"
core = "7.x"
package = "Multisite OG Features"
version = "7.x-1.0"
project = "wiki_og"
dependencies[] = "og"
dependencies[] = "communities"
dependencies[] = "features"
dependencies[] = "strongarm"
dependencies[] = "views"
dependencies[] = "wiki_core"
features[ctools][] = "strongarm:strongarm:1"
features[ctools][] = "views:views_default:3.0"
features[features_api][] = api:1
features[field][] = "node-wiki-og_group_ref"
features[field][] = "node-wiki-group_content_access"
features[og_features_permission][] = node:community:create wiki content
features[og_features_permission][] = node:community:delete any wiki content
features[og_features_permission][] = node:community:delete own wiki content
features[og_features_permission][] = node:community:update any wiki content
features[og_features_permission][] = node:community:update own wiki content
