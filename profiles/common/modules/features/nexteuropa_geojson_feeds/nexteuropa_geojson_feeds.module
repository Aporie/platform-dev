<?php
/**
 * @file
 * Code for the NextEuropa GeoJson Feeds feature.
 */

/**
 * Implements hook_enable().
 */
function communities_enable() {
  drupal_set_message(t('The GeoJSON Feeds feature is now active on your site.'));
}
/**
 * Implements hook_disable().
 */
function communities_disable() {
  drupal_set_message(t('The GeoJSON Feeds feature is now disabled on your site.'));
}
